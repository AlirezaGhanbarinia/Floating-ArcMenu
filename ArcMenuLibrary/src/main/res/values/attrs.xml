<?xml version="1.0" encoding="UTF-8"?>
<resources>

    <declare-styleable name="ArcMenu">
    	<attr name="menuChildSize" format="dimension|reference" />
    	<attr name="menuChildAnim" format="boolean" />
    	<attr name="menuClickAnim" format="boolean" />
    	<attr name="menuShadowElevation" format="boolean" />
		<attr name="menuImage" format="integer"/>
		<attr name="menuChildRadius" format="integer"/>
		<attr name="menuMarginRight" format="dimension"/>
		<attr name="menuMarginLeft" format="dimension"/>
		<attr name="menuMarginTop" format="dimension"/>
		<attr name="menuMarginBottom" format="dimension"/>
		<attr name="menuNormalColor" format="color"/>
		<attr name="menuPressedColor" format="color"/>
		<attr name="menuRippleColor" format="color"/>
        <attr name="menuGravity" format="integer">
            <enum name="Top_Left" value="0xF01" />
			<enum name="Top_Right" value="0xF02" />
			<enum name="Top_Middle" value="0xF03" />
			<enum name="Bottom_Left" value="0xF04" />
			<enum name="Bottom_Right" value="0xF05" />
			<enum name="Bottom_Middle" value="0xF06" />
			<enum name="Right_Middle" value="0xF07" />
			<enum name="Left_Middle" value="0xF08" />
			<enum name="Center" value="0xF09" />
    	</attr>
		<attr name="menuType" format="enum">
			<enum name="large" value="0" />
			<enum name="normal" value="1" />
			<enum name="mini" value="2" />
		</attr>
    </declare-styleable>

	<declare-styleable name="FloatingActionButton">
		<attr name="fab_colorPressed" format="color" />
		<attr name="fab_colorNormal" format="color" />
		<attr name="fab_colorRipple" format="color" />
		<attr name="fab_colorDisabled" format="color" />
		<attr name="fab_shadow" format="boolean" />
		<attr name="fab_type" format="enum">
			<enum name="large" value="0" />
			<enum name="normal" value="1" />
			<enum name="mini" value="2" />
		</attr>
	</declare-styleable>

</resources>